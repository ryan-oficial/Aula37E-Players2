@model Noticia
    @{
        ViewData["Title"] = "Noticia";
    }

    <form action="@Url.Action("Cadastrar")" method="POST" enctype="multipart/form-data">
        <label>ID</label>
        <input type="text" name="IdEquipe"/>

        <label>Titulo</label>
        <input type="text" name="Titulo"/>

        <label>Imagem</label>
        <input type="file" name="Imagem"/>

        <button type="submit">Cadastrar</button>
    </form>
    <table class="table table-striped table-responsive">
        <thead>
            <th>ID</th>
            <th>Titulo</th>
            <th>Foto</th>
        </thead>
        <tbody>
            @foreach(Noticia e in ViewBag.Noticias){
            <tr>
                <td>@e.IdNoticia</td>
                <td>@e.Titulo</td>
                <td>@e.Imagem</td>
            </tr>
        }
        </tbody>
    </table>